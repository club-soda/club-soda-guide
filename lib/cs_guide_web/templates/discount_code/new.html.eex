<section class="center w-80 w-75-l mv6-ns mv5">
  <h2 class="f4 lh4">New Discount Code</h2>

  <%= custom_render_autoform(
  @conn,
  :create,
  [CsGuide.DiscountCode],
  assigns: [changeset: @changeset],
  exclude: [:entry_id, :deleted, :venue_id],
  update_field: :entry_id,
  assoc_query: &query/1)
  %>

  <span><%= link "Back", to: discount_code_path(@conn, :index) %></span>
</section>
