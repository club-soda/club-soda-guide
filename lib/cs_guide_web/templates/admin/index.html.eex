<section class="center w-80 w-75-l mv6-ns mv5">
  <h1 class="f4 lh4">Admin Dashboard</h1>
  <div class="mv3">
    <div class="w-20-ns dib-ns">
      <%= link "Brands", to: brand_path(@conn, :index), class: "cs-black" %>
    </div>
    <div class="pv4 pv0-ns db dib-ns">
      <%= link "Download brands", to: csv_path(@conn, :export, data: "brands"), class: "btn-primary no-underline" %>
    </div>
    <%= link "Download brands with drinks", to: csv_path(@conn, :export, data: "brands-drinks"), class: "btn-primary no-underline" %>
