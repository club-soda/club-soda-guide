<section class="center w-80 w-75-l mv5">
  <h2 class="f4 lh4">Edit Brand</h2>


  <%= custom_render_autoform(@conn, :update,
    [{CsGuide.Resources.Brand, custom_labels: %{sold_amazon: "Sold on Amazon"}}], exclude: [:entry_id, :deleted, :drinks, :brand_images], custom_labels: %{sold_amazon: "Sold on Amazon YYYYY"}, path: brand_path(@conn, :update, @brand.name), assigns: [changeset: @changeset, brand: @brand], update_field: :entry_id, assoc_query: &query/1) %>

  <span><%= link "Back", to: brand_path(@conn, :index) %></span>
</section>
